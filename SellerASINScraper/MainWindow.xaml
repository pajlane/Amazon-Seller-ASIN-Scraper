<Window x:Class="SellerASINScraper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SellerASINScraper"
        mc:Ignorable="d"
        Title="Seller ASIN Scraper" Height="150" Width="300">
    <Grid>
        <TextBox x:Name="SellerIDx" HorizontalAlignment="Left" Height="23" Margin="83,23,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="190"/>
        <TextBlock HorizontalAlignment="Left" Height="23" Margin="10,23,0,0" TextWrapping="Wrap" Text="Seller ID" VerticalAlignment="Top" Width="84"/>

        <Button x:Name="Run" Content="Run" HorizontalAlignment="Left" VerticalAlignment="Top" Width="76" RenderTransformOrigin="7.835,18.767" Margin="83,79,0,0" IsDefault="True" Click="Run_Click"/>


    </Grid>
</Window>
